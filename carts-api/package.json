  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "dotenv-cli": "^7.3.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
